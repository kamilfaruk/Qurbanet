@model IEnumerable<Qurbanet.Models.DTOs.Organization.OrganizationListDto>
@{
    ViewData["Title"] = "Organizations";
}
<h1>Organizations</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Year</th>
            <th>Paid</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
        <tr>
            <td>@item.Name</td>
            <td>@item.Year</td>
            <td>@item.IsPaid</td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-primary btn-sm">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
<a asp-action="Create" class="btn btn-success">Add Organization</a>
